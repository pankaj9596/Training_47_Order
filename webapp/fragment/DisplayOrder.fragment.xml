<core:FragmentDefinition xmlns="sap.m" xmlns:core = "sap.ui.core" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form">
    <Dialog id="dlgDisplayOrder" title="Display Order" showHeader="false" contentWidth="30%" contentHeight="30%">
       	<f:SimpleForm id="SimpleFormDisplay354"
			editable="false"
			layout="ResponsiveGridLayout"
			title="Display Order"
			labelSpanXL="3"
			labelSpanL="3"
			labelSpanM="3"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			emptySpanS="0"
			columnsXL="1"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
			<f:content>
				<Label text="Order Id" />
				<Text text="{OrderID}" />

				<Label text="Customer Id" />
				<Text text="{CustomerID}" />

               <Label text="Order Date" />
                <Text text="{OrderDate}"></Text>
                
				<Label text="Employee Id" />
				 <Text text="{EmployeeID}"></Text>

				<Label text="Ship Name" />
				<Text text="{ShipName}"></Text>

                <Label text="Ship City" />
				<Text text="{ShipCity}"></Text>

				<Label text="Ship Country" />
				<Text text="{ShipCountry}"></Text>
			</f:content>
		</f:SimpleForm>
        <buttons>
            <Button type="Transparent" text="Close" press="onCloseDisplayDialogPress"/>
        </buttons>
    </Dialog>
</core:FragmentDefinition>

